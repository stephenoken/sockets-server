{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "semi":[2, "always"],
    "no-extra-semi":2,
    "no-undef":1,
    "no-unused-vars":1,
    "comma-dangle":[2,"never"],
    "no-cond-assign":2,
    "camelcase":[2,{"properties":"always"}]
  }
}
